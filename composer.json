{
    "name": "speedyspec/speedyspec-wp-hook-domain",
    "version": "1.1.2",
    "description": "SpeedySpec Rewrite of WordPress - Hook API Domain Package",
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=8.4"
    },
    "require-dev": {
        "pestphp/pest": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "SpeedySpec\\WP\\Hook\\Domain\\": "src/"
        },
        "files": [
            "functions/plugins.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SpeedySpec\\WP\\Hook\\Domain\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/docs",
            "/tests",
            "/.github",
            "/.git",
            "/.editorconfig",
            "/.gitattributes",
            "/.gitignore",
            "/phpunit.xml",
            "/CHANGELOG.md",
            "/.release-please-manifest.json",
            "/release-please-config.json"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
